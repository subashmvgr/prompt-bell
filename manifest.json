{
  "manifest_version": 3,
  "name": "PromptBell",
  "version": "1.0",
  "description": "Set simple reminders from your Chrome browser.",
  "permissions": ["alarms", "notifications", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
